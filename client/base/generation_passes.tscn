func _generate(iter: GridChunkIter) -> void:
	#print(\"generating \", iter.chunk_coord())
	
	if iter.chunk_coord().y >= 0:
		while iter.next():
			iter.set_cell(iter.local_coord().y % 2)
	else:
		while iter.next():
			iter.set_cell(iter.local_coord().x % 2)
